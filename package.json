{
  "name": "playwright-msw",
  "version": "0.0.5",
  "description": "A Mock Service Worker API for Playwright.",
  "main": "lib/server.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/valendres/playwright-msw"
  },
  "author": {
    "name": "Peter Weller",
    "url": "https://github.com/valendres"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "lint": "eslint \"src/**/*.ts\"",
    "postinstall": "husky install",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags",
    "prepare": "yarn clean && yarn build",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "dependencies": {
    "@mswjs/http-middleware": "^0.3.0",
    "body-parser": "^1.19.2",
    "express": "^4.17.3",
    "headers-polyfill": "^3.0.4",
    "node-fetch": "^2.6.7"
  },
  "peerDependencies": {
    "@playwright/test": ">=1.19.2",
    "msw": ">=0.35.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@playwright/test": "1.19.2",
    "@types/body-parser": "^1",
    "@types/eslint": "^8",
    "@types/express": "^4",
    "@types/node": "^17.0.21",
    "@types/node-fetch": "^2.6.1",
    "@types/prettier": "^2",
    "@types/rimraf": "^3",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.6",
    "msw": "0.35.0",
    "pinst": "^3.0.0",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2"
  },
  "packageManager": "yarn@3.2.0"
}
