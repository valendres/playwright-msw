{
  "name": "playwright-msw",
  "version": "0.0.9",
  "description": "A Mock Service Worker API for Playwright.",
  "main": "lib/server.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=14"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/valendres/playwright-msw"
  },
  "author": {
    "name": "Peter Weller",
    "url": "https://github.com/valendres"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags",
    "prepare": "yarn clean && yarn build"
  },
  "dependencies": {
    "@mswjs/http-middleware": "^0.3.0",
    "body-parser": "^1.19.2",
    "express": "^4.17.3",
    "headers-polyfill": "^3.0.4",
    "node-fetch": "^2.6.7"
  },
  "peerDependencies": {
    "@playwright/test": ">=1.20.0",
    "msw": ">=0.35.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.20.1",
    "@types/body-parser": "^1",
    "@types/express": "^4",
    "@types/node": "^17.0.21",
    "@types/node-fetch": "^2.6.1",
    "@types/rimraf": "^3",
    "msw": "0.39.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.2"
  }
}
